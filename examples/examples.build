<?xml version="1.0"?>
<project name="NAnt Examples" default="build">
    <target name="*">
        <nant target="${target::get-current-target()}">
            <buildfiles>
                <include name="**/*.build" />
                <exclude name="ScriptTask/script-sample.build" />
                <exclude name="examples.build" />
            </buildfiles>
        </nant>
    </target>
</project>
